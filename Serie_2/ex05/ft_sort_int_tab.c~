
//void ft_swap(int *a, int *b);
/*[7, 4, 12, 9, 11, 24]*/


void ft_sort_int_tab(int *tab, int size)
{
  int i;
  int j;

  j = 0;
  i = 0;
  while (i != size)
  {
    while (j != size)
    {
      if (tab[j] > tab[i])
      {
	int c = tab[j];
	tab[j] = tab[i];
	tab[i] = c;
	//ft_swap(&tab[j], &tab[i]);
      }
      j++;
    }
    j = 0;
    i++;
  }  
}
