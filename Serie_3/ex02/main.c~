#include <stdio.h>

int ft_str_is_alpha(char *str);

int main()
{
  char test[] = "";
  int result;
  result = ft_str_is_alpha(test);
  if (result)
  {
    printf("Ce mot ne contient que des caracteres alphabetiques");
  }
  else
  {
    printf("Ce mot ne contient pas que des caractÃ¨res alphabetiques");
  }
  return 0;
}
